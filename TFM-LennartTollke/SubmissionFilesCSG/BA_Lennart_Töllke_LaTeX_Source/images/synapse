	<script excluded-from-delay-js type="text/javascript">
		function manipulateElemmentIfExist(element, visibility)
		{
			if(visibility == 'hide') {
				jQuery(element).hide();
			} else if(visibility == 'show'){
				jQuery(element).show();
			}
		}

		function modifyLinksByUserState()
		{
			jQuery('.js-main-cta').each(function(){
				jQuery(this).children().find('a').attr('href', $(this).data('log-link'));
				jQuery(this).children().find('a .elementor-button-text').html($(this).data('log-text'));
			});

			jQuery('.js-main-cta-static').each(function(){
				jQuery(this).attr('href', $(this).data('log-link'));
				jQuery(this).html($(this).data('log-text'));
			});	
		}

		function manageClassesByUserState(result, $, appSubDomain = 'app')
		{
			if(!result) {
				return false;
			}

			if(result.state && result.state != 's') {
				manipulateElemmentIfExist('.js-not-subscribed-show', 'show');
				manipulateElemmentIfExist('.js-not-subscribed-hide', 'hide');
			} else {
				manipulateElemmentIfExist('.js-not-subscribed-show', 'hide');
				manipulateElemmentIfExist('.js-not-subscribed-hide', 'show');
			}

			if(result.state && result.state != 'v') {

				if(result.state == 's'){
					manipulateElemmentIfExist('.js-subscribed-show', 'show');
					manipulateElemmentIfExist('.js-subscribed-hide', 'hide');
				}

				manipulateElemmentIfExist('.js-logged-in-show', 'show');
				manipulateElemmentIfExist('.js-logged-in-hide', 'hide');

				modifyLinksByUserState();

			} else {
				manipulateElemmentIfExist('.js-logged-in-show', 'hide');
				manipulateElemmentIfExist('.js-logged-in-hide', 'show');
				manipulateElemmentIfExist('.js-subscribed-show', 'hide');
				manipulateElemmentIfExist('.js-subscribed-hide', 'show');
			}

			if(result.state && result.state == 's' && result.user.access_level == 'premium_active') { 
				let pricingLinks = [];
				
				$.each($('a'), function(key, e){
					if($(e).attr('href')){
						if ($(e).attr('href').indexOf('lecPricingCurrency') != -1) {
							pricingLinks.push($(e));
						}
					}
				});
				
				// if( ($('.lecPrice').length ||
				// 	$('.lecCurrencySymbol').length ||
				// 	pricingLinks.length) &&
				// 	window.elementorFrontend.config.environmentMode.edit == false
				// 	) {
				// 	window.location.replace('https://app.lecturio.com/account/payments');
				// }
			}

			if(appSubDomain.length > 0 && appSubDomain !== 'app') {
				$.each($('.js-logged-in-redirect').find('a'), function(index, el) {
					let hrefAttr = $(el).attr('href');
					$(el).attr('href', hrefAttr.replace('app.', appSubDomain + '.') );
				});
				
				$.each($('.js-logged-in-redirect'), function(index, el) {
					let hrefAttr = $(el).attr('href');
					if(hrefAttr) {
						$(el).attr('href', hrefAttr.replace('app.', appSubDomain + '.') );
					}
				});
			}
			
		}

		jQuery(document).ready(function($)
		{
			manageClassesByUserState(JSON.parse(sessionStorage.getItem('userState')), $, sessionStorage.getItem('appSubDomain') );
		});
	</script>
	    <script type="text/javascript">
        function stopPopup(modalIds) {

            if (!modalIds) {
                return;
            }

            const targetNode = document.body;

            const config = {attributes: true, childList: true, subtree: true};

            const callback = function (mutationsList, observer) {

                for (let mutation of mutationsList) {
                    if (mutation.type === 'childList') {

                        if (document.getElementsByClassName("elementor-popup-modal").length) {

                            let mutateModalSelector = jQuery(mutation.addedNodes[0]).attr('id');

                            if (mutateModalSelector) {
                                let mutateModalId = mutateModalSelector.match(/.*-(.*)/);
                                mutateModalId = parseInt(mutateModalId[1]);

                                if (modalIds.indexOf(mutateModalId) != -1) {
                                    document.getElementById('elementor-popup-modal-' + mutateModalId).style.display = 'none';
                                }
                            }

// 							modalIds.forEach(function(modId)
// 							{
// 								document.getElementById('elementor-popup-modal-' + modId).style.display='none';	
// 							});
                            //stop observer
// 							observer.disconnect();
                        }
                    }
                }
            };
            const observer = new MutationObserver(callback);
            observer.observe(targetNode, config);
        }
    </script>
	        <script type="text/javascript">
            const popupSettings = '[{"id":137987,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":137980,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":137970,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":137899,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":132885,"user_state":"default","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":132818,"user_state":"u_s","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":132802,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":132789,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":131410,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":131364,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":125953,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":125724,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":125720,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":104410,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":94843,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":84250,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":84237,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":62170,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":62167,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":38882,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":38876,"user_state":"u_s","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":35088,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":35084,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":29474,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":28360,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":21394,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":21152,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":20933,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":19497,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":15583,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":15494,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":13953,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":8708,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":8101,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6530,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6254,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6245,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6236,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6147,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6138,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6053,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6047,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6041,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6032,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6026,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":6015,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":5959,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""},{"id":5641,"user_state":"","country_code_match":"","country_codes":"","user_id_match":"","user_ids":"","user_credentials_match":"","user_credentials":""}]';

            function managePopupsByUserState(userJson, popupSettings) {
                // const userJson = result;

                let arrModalsForHiding = [];

                JSON.parse(popupSettings).forEach(function (popup) {
                    //1 not exist
                    //2 exist but not verified
                    //3 verified
                    let settingModalStatus = '1';

                    if (!userJson) {
                        userJson = '';
                    }

                    if (userJson.state == popup.user_state) {
                        arrModalsForHiding.push(popup.id);
                        settingModalStatus = 3;
                    } else if ((userJson.state == 'u' || userJson.state == 's') && popup.user_state == 'u_s') {
                        arrModalsForHiding.push(popup.id);
                        settingModalStatus = 3;
                    } else if (popup.user_state == 'default') {
                        settingModalStatus = 1;
                    } else {
                        settingModalStatus = 2;
                    }

                    if (settingModalStatus != 2) {
                        if (popup.country_codes && popup.country_code_match) {

                            let arrCountryCodes = popup.country_codes.replace(/\s/g, '').split(',');

                            if (popup.country_code_match == 'match') {
                                if (arrCountryCodes.includes(userJson.user.countryCode)) {
                                    if (!arrModalsForHiding.includes(popup.id)) {
                                        arrModalsForHiding.push(popup.id);
                                    }
                                    settingModalStatus = 3;

                                } else {
                                    arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                    settingModalStatus = 2;
                                }
                            }

                            if (popup.country_code_match == 'not_match') {
                                if (!arrCountryCodes.includes(userJson.user.countryCode)) {

                                    if (!arrModalsForHiding.includes(popup.id)) {
                                        arrModalsForHiding.push(popup.id);
                                    }
                                    settingModalStatus = 3;
                                } else {
                                    arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                    settingModalStatus = 2;
                                }
                            }
                        } else {
                            settingModalStatus = 1;
                        }
                    }

                    if (settingModalStatus != 2) {
                        if (popup.user_ids && popup.user_id_match) {

                            let arrUserIds = popup.user_ids.replace(/\s/g, '').split(',');

                            if (popup.user_id_match == 'match') {
                                if (arrUserIds.includes(userJson.user.id)) {
                                    if (!arrModalsForHiding.includes(popup.id)) {
                                        arrModalsForHiding.push(popup.id);
                                    }
                                    settingModalStatus = 3;

                                } else {
                                    arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                    settingModalStatus = 2;
                                }

                            }

                            if (popup.user_id_match == 'not_match') {
                                if (!arrUserIds.includes(userJson.user.id)) {

                                    if (!arrModalsForHiding.includes(popup.id)) {
                                        arrModalsForHiding.push(popup.id);
                                    }
                                    settingModalStatus = 3;
                                } else {
                                    arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                    settingModalStatus = 2;
                                }
                            }
                        } else {
                            settingModalStatus = 1;
                        }
                    }

                    if (settingModalStatus != 2) {
                        if (popup.user_credentials && popup.user_credentials_match) {

                            let arrUserCredentials = popup.user_credentials.replace(/\s/g, '').split(',');

                            if (popup.user_credentials_match == 'match') {
                                for (let i = 0; i < arrUserCredentials.length; i++) {
                                    if (userJson.credentials.includes(arrUserCredentials[i])) {
                                        if (!arrModalsForHiding.includes(popup.id)) {
                                            arrModalsForHiding.push(popup.id);
                                        }
                                        settingModalStatus = 3;
                                        break;
                                    } else {
                                        arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                        settingModalStatus = 2;
                                    }
                                }
                            }

                            if (popup.user_credentials_match == 'not_match') {
                                for (let i = 0; i < arrUserCredentials.length; i++) {
                                    if (!userJson.credentials.includes(arrUserCredentials[i])) {
                                        if (!arrModalsForHiding.includes(popup.id)) {
                                            arrModalsForHiding.push(popup.id);
                                        }
                                        settingModalStatus = 3;
                                        break;
                                    } else {
                                        arrModalsForHiding.splice(arrModalsForHiding.indexOf(popup.id), 1);
                                        settingModalStatus = 2;
                                    }
                                }
                            }
                        } else {
                            settingModalStatus = 1;
                        }
                    }
                });
                stopPopup(arrModalsForHiding);
            }


            // jQuery.ajax({
            //     type: 'GET',
            //     url: "",
            //     xhrFields: {
            //         withCredentials: true
            //     },
            //     success: function(result) {
            // 		managePopupsByUserState(result, popupSettings);
            // 	}
            // });

            managePopupsByUserState(JSON.parse(sessionStorage.getItem('userState')), popupSettings);
        </script>
